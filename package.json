{
  "name": "tribeca-cep",
  "version": "0.0.1",
  "description": "A simple complex event processing system",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/src/main.js",
    "start-dev": "cd src && ts-node main.ts --inspect=0.0.0.0:9229",
    "start-watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage",
    "publish-code-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3beca/cep.git"
  },
  "keywords": [
    "cep",
    "nodejs",
    "iot"
  ],
  "author": "juanjofp, francescorivola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3beca/cep/issues"
  },
  "homepage": "https://github.com/3beca/cep#readme",
  "devDependencies": {
    "@types/agenda": "^4.1.0",
    "@types/convict": "^5.2.2",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.9",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/pino": "^6.3.6",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "codecov": "^3.8.1",
    "eslint": "^7.22.0",
    "eslint-plugin-filenames": "^1.3.2",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "agenda": "^3.1.0",
    "ajv": "^7.1.1",
    "ajv-errors": "^2.0.1",
    "ajv-formats": "^1.5.1",
    "convict": "^6.0.1",
    "escape-string-regexp": "^4.0.0",
    "fastify": "^3.14.0",
    "fastify-bearer-auth": "^5.1.0",
    "fastify-cors": "^5.2.0",
    "fastify-metrics": "^7.1.0",
    "fastify-swagger": "^4.4.2",
    "geolib": "^3.3.1",
    "liquidjs": "^9.23.2",
    "make-promises-safe": "^5.1.0",
    "mongodb": "^3.6.4",
    "node-fetch": "^2.6.1",
    "prom-client": "^13.1.0",
    "save": "^2.4.0"
  }
