{
  "name": "tribeca-cep",
  "version": "0.0.1",
  "description": "A simple complex event processing system",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "cd build && node main.js",
    "start-dev": "cd src && ts-node main.ts --inspect=0.0.0.0:9229",
    "start-watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage",
    "publish-code-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3beca/cep.git"
  },
  "keywords": [
    "cep",
    "nodejs"
  ],
  "author": "juanjofp, francescorivola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3beca/cep/issues"
  },
  "homepage": "https://github.com/3beca/cep#readme",
  "devDependencies": {
    "@types/convict": "^5.2.1",
    "@types/jest": "^25.2.2",
    "@types/mongodb": "^3.5.16",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "codecov": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "jest": "^25.5.4",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "ajv-errors": "^1.0.1",
    "convict": "^6.0.0",
    "escape-string-regexp": "^4.0.0",
    "fastify": "^2.14.1",
    "fastify-cors": "^3.0.3",
    "fastify-swagger": "^2.5.1",
    "geolib": "^3.2.1",
    "make-promises-safe": "^5.1.0",
    "mongodb": "^3.5.7",
    "node-fetch": "^2.6.0"
  }
}
