{
  "name": "tribeca-cep",
  "version": "0.0.1",
  "description": "A simple complex event processing system",
  "main": "build/main.js",
  "scripts": {
    "build": "babel src -d build",
    "start": "cd build && node main.js",
    "start-dev": "cd src && babel-node --inspect=0.0.0.0:9229 main.js",
    "start-watch": "nodemon --inspect=0.0.0.0:9229 -L src/main.js  --exec babel-node",
    "lint": "eslint src",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3beca/cep.git"
  },
  "keywords": [
    "cep",
    "nodejs"
  ],
  "author": "juanjofp, francescorivola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3beca/cep/issues"
  },
  "homepage": "https://github.com/3beca/cep#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-filenames": "^1.3.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3"
  },
  "dependencies": {
    "bson": "^4.0.2",
    "convict": "^5.1.0",
    "fastify": "^2.9.0",
    "fastify-swagger": "^2.4.0",
    "make-promises-safe": "^5.1.0"
  }
}
