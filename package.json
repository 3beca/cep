{
  "name": "tribeca-cep",
  "version": "0.0.1",
  "description": "A simple complex event processing system",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/src/main.js",
    "start-dev": "cd src && ts-node main.ts --inspect=0.0.0.0:9229",
    "start-watch": "nodemon -L src/main.ts --inspect=0.0.0.0:9229 --exec ts-node",
    "lint": "eslint . --ext .ts",
    "test": "jest --watchAll",
    "test-ci": "jest --collect-coverage",
    "publish-code-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3beca/cep.git"
  },
  "keywords": [
    "cep",
    "nodejs",
    "iot"
  ],
  "author": "juanjofp, francescorivola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3beca/cep/issues"
  },
  "homepage": "https://github.com/3beca/cep#readme",
  "devDependencies": {
    "@types/agenda": "^2.0.9",
    "@types/convict": "^5.2.1",
    "@types/jest": "^26.0.15",
    "@types/mongodb": "^3.5.31",
    "@types/node": "^14.14.0",
    "@types/node-fetch": "^2.5.7",
    "@types/pino": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codecov": "^3.8.0",
    "eslint": "^6.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "jest": "^25.5.4",
    "nock": "^13.0.4",
    "nodemon": "^2.0.6",
    "ts-jest": "^25.5.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "agenda": "^3.1.0",
    "ajv": "^6.12.6",
    "ajv-errors": "^1.0.1",
    "convict": "^6.0.0",
    "escape-string-regexp": "^4.0.0",
    "fastify": "^3.5.0",
    "fastify-bearer-auth": "^5.0.2",
    "fastify-cors": "^4.1.0",
    "fastify-metrics": "^6.0.2",
    "fastify-swagger": "^3.4.0",
    "geolib": "^3.3.1",
    "liquidjs": "^9.16.1",
    "make-promises-safe": "^5.1.0",
    "mongodb": "^3.6.2",
    "node-fetch": "^2.6.1",
    "prom-client": "^12.0.0"
  }
}
