{
    "title": "List Querystring Schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "page": {
            "type": "integer",
            "minimum": 1,
            "default": 1
        },
        "pageSize": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 10
        },
        "search": {
            "type": "string"
        }
    },
    "required": ["page", "pageSize"]
}